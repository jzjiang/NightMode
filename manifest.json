{
  "manifest_version": 2,
  "name": "Night Mode",
  "description": "beware the spooky night!!",
  "version": "1.0",
  
  "web_accessible_resources": ["dancing-skeleton.gif"],

  "browser_action": {
    "default_icon": "icon.png",
	"default_title": "spoOOooky!!"
  },
  
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
	  "css": ["style.css"],
      "js": ["jquery.min.js", "content-script.js"],
	  "run_at": "document_start"
    }
  ],
  
   "permissions": 
  [
  "activeTab",
  "https://ajax.googleapis.com/"
  ]
}
